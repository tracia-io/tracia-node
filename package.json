{
  "name": "tracia",
  "version": "0.3.11",
  "description": "TypeScript/JavaScript SDK for Tracia - store, test, and trace LLM prompts",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.mts",
        "default": "./dist/index.mjs"
      },
      "require": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      }
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "tsup --watch",
    "build": "tsup",
    "test": "vitest run --coverage",
    "test:watch": "vitest",
    "lint": "eslint src tests",
    "typecheck": "tsc --noEmit",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "llm",
    "prompt",
    "tracing",
    "observability",
    "openai",
    "chatgpt",
    "anthropic",
    "claude",
    "google",
    "gemini",
    "bedrock",
    "aws",
    "ai",
    "vercel-ai-sdk"
  ],
  "author": "Tracia",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tracia-io/tracia-node.git"
  },
  "homepage": "https://tracia.io",
  "bugs": {
    "url": "https://github.com/tracia-io/tracia-node/issues"
  },
  "peerDependencies": {
    "@ai-sdk/amazon-bedrock": ">=1.0.0",
    "@ai-sdk/anthropic": ">=3.0.0",
    "@ai-sdk/google": ">=3.0.0",
    "@ai-sdk/openai": ">=3.0.0",
    "ai": ">=6.0.0"
  },
  "peerDependenciesMeta": {
    "ai": {
      "optional": true
    },
    "@ai-sdk/openai": {
      "optional": true
    },
    "@ai-sdk/anthropic": {
      "optional": true
    },
    "@ai-sdk/google": {
      "optional": true
    },
    "@ai-sdk/amazon-bedrock": {
      "optional": true
    }
  },
  "devDependencies": {
    "@ai-sdk/anthropic": "^3.0.33",
    "@ai-sdk/google": "^3.0.18",
    "@ai-sdk/openai": "^3.0.23",
    "@types/node": "^20.10.0",
    "@typescript-eslint/eslint-plugin": "^6.13.0",
    "@typescript-eslint/parser": "^6.13.0",
    "ai": "^6.0.64",
    "eslint": "^8.55.0",
    "tsup": "^8.0.0",
    "typescript": "^5.3.0",
    "vitest": "^1.0.0"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
